function main () {
  x := {
    w: 2
  };

  if (false) {
    x.z := 0;
  },else {
    x.z := 1;
  };

  y := {};

  if (x.z = -1) {
    y.z := -1;
  },else if (x.z = 0) {
    y.z := 0;
  },else if (x.z = 1) {
    y.z := 3;
  };

  if (y.z = 3) {
    return x.w + x.z + y.z;
  };

  y.z := -1;

  return y.z
}
